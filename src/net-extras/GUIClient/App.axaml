<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:GUIClient"
             x:Class="GUIClient.App">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme Mode="Dark"/>
        <StyleInclude Source="avares://GUIClient/Styles/Icons.axaml" />
        <StyleInclude Source="avares://GUIClient/Styles/WindowStyles.axaml" />
    </Application.Styles>
</Application>
